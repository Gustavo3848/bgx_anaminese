<div class="container">
  <div class="row">
    <div class="col mt-5">
      <div class="card">
        <div class="card-body" style="background-color: lightyellow;">
          <h5 class="card-title text-center">Anamineses</h5>
          <div class="d-grid gap-1">
            <input type="text" id="search-input" placeholder="buscar">
          </div>
          <div class="list-group mt-2 table-striped" id="search-data">
            <% itens.forEach(function(item) { %>
            <a href="/anaminese/visualizar/<%= item.id %>" class="list-group-item list-group-item-action"><%= item.nome %></a>
            <% }); %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
